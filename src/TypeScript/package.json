{
  "name": "@equantic/linq",
  "version": "0.1.0",
  "description": "eQuantic Linq for JavaScript",
  "keywords": [
    "eQuantic",
    "Linq"
  ],
  "homepage": "https://github.com/eQuantic/core-linq#readme",
  "bugs": {
    "url": "https://github.com/eQuantic/core-linq/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/eQuantic/core-linq.git"
  },
  "license": "ISC",
  "author": "eQuantic Tech",
  "type": "commonjs",
  "main": "./",
  "types": "./",
  "directories": {
    "lib": "lib"
  },
  "scripts": {
    "test": "jest --config jestconfig.json",
    "build": "tsc",
    "format": "prettier --write \"src/**/*.{ts,js}\"",
    "lint": "tslint -p tsconfig.json",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint && mv lib/* ./ && rm -rf lib",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src"
  },
  "dependencies": {
    "flat": "^6.0.1",
    "qs": "^6.13.1"
  },
  "devDependencies": {
    "@types/flat": "^5.0.5",
    "@types/node": "^22.10.2",
    "@types/qs": "^6.9.17",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^5.7.2"
  }
}
